<resources>

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!-- API 11 theme customizations can go here. -->
    </style>

    <style name="MeneameTheme.Overlay" parent="@style/MeneameTheme">
        <item name="android:windowActionBarOverlay">true</item>
    </style>
    
    <style name="MeneameTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MeneameActionBar</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.MeneameTheme</item>
    </style>

</resources>